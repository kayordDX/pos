		"@kayord/tw-plugin": "^1.0.1",
		"@kayord/ui": "^0.6.3",
		"@sveltejs/adapter-node": "^4.0.1",
		"@sveltejs/kit": "^2.5.2",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@tanstack/svelte-query": "^5.25.0",
		"@types/qs": "^6.9.12",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^7.1.1",
		"@typescript-eslint/parser": "^7.1.1",
		"autoprefixer": "^10.4.18",
