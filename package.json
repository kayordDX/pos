		"@kayord/ui": "^0.4.5",
		"@sveltejs/adapter-node": "^4.0.1",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@tanstack/svelte-query": "^5.20.5",
		"@types/qs": "^6.9.11",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"autoprefixer": "^10.4.17",
